{
  "name": "raz/framework",
  "autoload": {
    "psr-4": {
      "Raz\\": "src/",
      "Raz\\Lib\\": "src/lib",
      "Raz\\Lib\\HTTP\\": [
        "src/lib/error",
        "src/lib/http"
      ],
      "Raz\\Lib\\Routing\\": "src/lib/router",
      "Raz\\Lib\\Interfaces\\": "src/lib/interfaces",
      "Raz\\Lib\\Injector\\": "src/lib/injector"
    }
  },
  "authors": [
    {
      "name": "razmiO",
      "email": "thomas.cuesta31@gmail.com"
    }
  ],
  "version": "1.0.0",
  "license": "MIT",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=7.4",
    "php-di/php-di": "^7.0",
    "vlucas/phpdotenv": "^5.6"
  },
  "type": "library",
  "description": "A framework I made for a school project with a modular architecture and dependency injection.",
  "keywords": [
    "php",
    "framework",
    "raz",
    "modular",
    "dependency-injection",
    "routing"
  ]
}
